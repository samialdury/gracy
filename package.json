{
    "name": "gracy",
    "version": "4.0.0",
    "description": "Execute custom cleanup function before Node.js exits",
    "keywords": [
        "graceful",
        "shutdown",
        "cleanup",
        "exit",
        "signal",
        "process",
        "node",
        "nodejs",
        "close",
        "terminate"
    ],
    "homepage": "https://github.com/samialdury/gracy#readme",
    "bugs": {
        "url": "https://github.com/samialdury/gracy/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/samialdury/gracy.git"
    },
    "license": "MIT",
    "author": "Sami Al-Dury",
    "type": "module",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./package.json": "./package.json"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "./dist",
        "./src"
    ],
    "devDependencies": {
        "@commitlint/cli": "18.4.3",
        "@samialdury/config": "6.2.0",
        "@types/node": "20.10.4",
        "eslint": "8.55.0",
        "glob": "10.3.10",
        "husky": "8.0.3",
        "lint-staged": "15.2.0",
        "prettier": "3.1.1",
        "semantic-release": "22.0.10",
        "tsup": "8.0.1",
        "tsx": "4.6.2",
        "typescript": "5.3.3"
    },
    "engines": {
        "node": ">=20",
        "pnpm": ">=8"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
