{
	"name": "gracy",
	"version": "1.0.2",
	"description": "Execute custom cleanup functions before Node.js exits",
	"author": "Sami Al-Dury",
	"license": "MIT",
	"homepage": "https://github.com/samialdury/gracy#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/samialdury/gracy.git"
	},
	"bugs": {
		"url": "https://github.com/samialdury/gracy/issues"
	},
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"./dist"
	],
	"types": "./dist/index.d.ts",
	"engines": {
		"node": ">=18"
	},
	"devDependencies": {
		"@changesets/changelog-git": "0.1.14",
		"@changesets/cli": "2.26.1",
		"@commitlint/cli": "17.5.1",
		"@commitlint/config-conventional": "17.4.4",
		"@types/node": "^18.15.10",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"@vitest/coverage-c8": "0.29.8",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.3",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "8.0.3",
		"lint-staged": "13.2.1",
		"prettier": "^2.8.6",
		"tsup": "6.7.0",
		"typescript": "5.0.4",
		"vitest": "0.29.8"
	},
	"keywords": [
		"graceful",
		"shutdown",
		"cleanup",
		"exit",
		"signal",
		"process",
		"node",
		"nodejs",
		"close",
		"terminate"
	]
}
